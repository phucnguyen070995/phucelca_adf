							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Write_All_Files_Full_Ext",
						"type": "DatasetReference",
						"parameters": {
							"fileName": {
								"value": "@pipeline().parameters.fileName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"fileName": {
				"type": "string",
				"defaultValue": "fileName"
			}
		},
		"folder": {
			"name": "Project"
		},
