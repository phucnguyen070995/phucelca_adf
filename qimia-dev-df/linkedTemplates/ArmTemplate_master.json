{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "qimia-dev-df"
		},
		"ABLB_phucelcablob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABLB_phucelcablob'"
		},
		"ADLS_phucelcadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_phucelcadls'"
		},
		"ASQL_phucelca_Entertainment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_phucelca_Entertainment'"
		},
		"ASQL_phucelca_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_phucelca_database'"
		},
		"SQL_phucelca_Entertainment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_phucelca_Entertainment'"
		},
		"ADLS_phucelcadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://phucelcadls.dfs.core.windows.net"
		},
		"Http_lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "qimia-dev-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABLB_phucelcablob_connectionString": {
						"value": "[parameters('ABLB_phucelcablob_connectionString')]"
					},
					"ADLS_phucelcadls_accountKey": {
						"value": "[parameters('ADLS_phucelcadls_accountKey')]"
					},
					"ASQL_phucelca_Entertainment_connectionString": {
						"value": "[parameters('ASQL_phucelca_Entertainment_connectionString')]"
					},
					"ASQL_phucelca_database_connectionString": {
						"value": "[parameters('ASQL_phucelca_database_connectionString')]"
					},
					"SQL_phucelca_Entertainment_connectionString": {
						"value": "[parameters('SQL_phucelca_Entertainment_connectionString')]"
					},
					"ADLS_phucelcadls_properties_typeProperties_url": {
						"value": "[parameters('ADLS_phucelcadls_properties_typeProperties_url')]"
					},
					"Http_lego_properties_typeProperties_url": {
						"value": "[parameters('Http_lego_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}